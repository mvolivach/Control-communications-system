    <div class="form-info">
      <label>Прізвище
        <input type="text" name="surname">
      </label>
      <label>По-батькові
        <input type="text" name="patronymic">
      </label>
      <label>Ім'я
        <input type="text" name="name">
      </label>
      <label>Номер телефону
        <input type="text" name="phone">
      </label>
